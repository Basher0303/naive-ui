{
  "compilerOptions": {
    "paths": {
      "naive-ui": ["./src"]
    },
    "strict": true,
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "jsxImportSource": "vue",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "module": "ES6",
    "moduleResolution": "Node",
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "composite": true,
    "target": "ES6",
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM"]
  }
}
